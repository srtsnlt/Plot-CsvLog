{
    // Plot settings
    "PlotEnd": "0:00:10.000",
    "PlotSpan": "0:00:00.100",

    // Input csv file settings
    "InFileName": "in.csv",

    "TimeColumnName": "Time",
    "TimeFormat": "h\\:mm\\:ss\\.fff",

    "NameColumnName": "Name",
    "NameReplaceRegexFrom": "^(.*)$",
    "NameReplaceRegexTo": "$1",

    "StateColumnName": "State",
    "StateRegexStart": "^Start$",
    "StateRegexEnd": "^End$"
}
